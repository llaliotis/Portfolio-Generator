{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "client/build"
      }
    }
  ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/app.py"
      },
      {
        "src": "/(.*)",
        "dest": "/client/build/$1"
      }
    ]
  }
  